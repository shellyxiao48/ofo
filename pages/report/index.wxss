/* pages/report/index.wxss */
page{
  background: #f5f5f5;

}
.container{
  width: 100%;
  height: 100vh;
}
.o_label{
  padding: 30rpx 40rpx;
  font-size: 32rpx;
}
.o_cells{
  margin-top: 25rpx;
  padding: 40rpx;
  background-color: #fff;

}
.checkboxwrapper{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.checkboxwrapper .checkbox{
  width: 45%;
  border: 1px solid #b6db07;
  padding: 30rpx 1% ; 
  border-radius: 6px;
  text-align: center;
  margin-top: 30rpx;

}
.checkboxwrapper .checkbox:nth-of-type(odd){
  margin-right: 3%;
}
.checkboxwrapper .checkbox:nth-of-type(1),.checkboxwrapper .checkbox:nth-of-type(2){
  margin-top: 0;
}

.images{
  position: relative;
  width: 140rpx;
  height: 140rpx;
  margin: 15rpx;
  display: inline-block;
}
.images image{
   width: 100%;
   height: 100%;
  vertical-align:middle;
   
}

.cancel{
  position: absolute;
top:-15rpx;
right:-15rpx;
font-size:30rpx;

}

.fileInput{
  width: 140rpx;
  height: 140rpx;
  border: 1px solid #ccc;
  margin: 15rpx;  
  line-height: 140rpx;
  text-align: center;
  display: inline-block;
  vertical-align:middle;
  font-size: 32rpx;
  color: #ccc;
}


button{
  width: 90%;
  margin: 30rpx auto;
}